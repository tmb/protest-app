<p id="notice"><%= notice %></p>

<h1>Protests</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Location</th>
      <th>Description</th>
      <th>What to bring</th>
      <th>Organizer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @protests.each do |protest| %>
      <tr>
        <td><%= protest.name %></td>
        <td><%= protest.location %></td>
        <td><%= protest.description %></td>
        <td><%= protest.what_to_bring %></td>
        <td><%= protest.organizers %></td>
        <td><%= link_to 'Show', protest %></td>
        <td><%= link_to 'Edit', edit_protest_path(protest) %></td>
        <td><%= link_to 'Destroy', protest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Protest', new_protest_path %>
